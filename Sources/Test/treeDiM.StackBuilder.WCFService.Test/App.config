<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
  <!--Logging-->
  <log4net>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RichTextBoxAppender" />
    </root>
    <appender name="RichTextBoxAppender" type="log4net.Appender.RichTextBoxAppender, RichTextBoxAppender">
      <mapping>
        <level value="WARN" />
        <foreColor value="Blue" />
        <isItalic value="true" />
      </mapping>
      <mapping>
        <level value="INFO" />
        <foreColor value="ControlText" />
      </mapping>
      <mapping>
        <level value="DEBUG" />
        <foreColor value="DarkGreen" />
      </mapping>
      <mapping>
        <level value="FATAL" />
        <foreColor value="Black" />
        <backColor value="Red" />
        <isBold value="true" />
        <isItalic value="true" />
        <pointSize value="10" />
      </mapping>
      <mapping>
        <level value="ERROR" />
        <foreColor value="Red" />
        <isBold value="false" />
        <pointSize value="10" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline%exception" />
      </layout>
    </appender>
    <system.diagnostics>
      <trace autoflush="false" indentsize="3" />
    </system.diagnostics>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IStackBuilder" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:5512/StackBuilder.svc" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IStackBuilder" contract="SB_SR.IStackBuilder"
        name="BasicHttpBinding_IStackBuilder" />
    </client>
  </system.serviceModel>
  <system.net>
    <defaultProxy useDefaultCredentials="true"/>
  </system.net>
</configuration>
